# print special strings
print("a\"b\\c\nd") # top level printed strings do not receive any escaping.

var d = dict()
d.add("a\"b", "c\\d\ne") # nested strings do receive escaping
print(d)

var l = []
l.push("a\"b", "c\\d\ne") # nested strings do receive escaping
print(l)
